function exclaim(str) {
  if (str.indexOf("!") !== str.length - 1) {
    return str + "!";
  } else {
    return str;
  }
}exclaim("FishFry")
 console.log(exclaim("Turkey Bacon!"))
    console.log(exclaim("Bacon!!"))
    console.log(exclaim("Beef Bacon!!!"))


